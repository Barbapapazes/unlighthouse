{
  "name": "@unlighthouse/core",
  "type": "module",
  "version": "0.3.10",
  "description": "Scan your entire website with Google Lighthouse.",
  "keywords": [
    "lighthouse",
    "audit",
    "seo",
    "performance",
    "speed"
  ],
  "homepage": "https://github.com/harlan-zw/unlighthouse#readme",
  "bugs": {
    "url": "https://github.com/harlan-zw/unlighthouse/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/unlighthouse.git",
    "directory": "packages/core"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "pnpm": {
    "packageExtensions": {
      "puppeteer-cluster": {
        "peerDependencies": {
          "puppeteer-core": "*"
        },
        "peerDependenciesMeta": {
          "puppeteer": {
            "optional": true
          }
        }
      }
    }
  },
  "peerDependenciesMeta": {
    "puppeteer": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/cli": "^2.15.8",
    "@nuxt/utils": "^2.15.8",
    "@unlighthouse/client": "link:../client",
    "@unrouted/core": "^0.4.11",
    "@unrouted/fetch": "^0.4.11",
    "@unrouted/plugins": "^0.4.11",
    "@unrouted/preset-api": "^0.4.11",
    "@unrouted/preset-node": "0.4.11",
    "axios": "^0.26.1",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "chrome-launcher": "^0.15.0",
    "consola": "^2.15.3",
    "defu": "^6.0.0",
    "execa": "^6.1.0",
    "fs-extra": "^10.0.1",
    "globby": "^13.1.1",
    "hookable": "^5.1.1",
    "launch-editor": "^2.3.0",
    "lighthouse": "^9.5.0",
    "mlly": "^0.4.3",
    "object-hash": "^3.0.0",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-core": "^13.5.2",
    "regexparam": "^2.0.0",
    "sanitize-filename": "^1.6.3",
    "sitemapper": "^3.2.2",
    "slugify": "^1.6.5",
    "ufo": "^0.8.3",
    "unconfig": "^0.3.2",
    "unctx": "^1.1.4",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.23",
    "@types/object-hash": "^2.2.1",
    "@types/ws": "^8.5.3",
    "lodash-es": "^4.17.15"
  }
}
